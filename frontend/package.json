{
  "name": "wissenslandkarte",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "format": "prettier --config .prettierrc --write .",
    "format-check": "prettier --config .prettierrc --check .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
    "lint-fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --max-warnings 0 --fix .",
    "db:remove": "rm -f prisma/data.db",
    "db:setup": "prisma generate && prisma migrate deploy && prisma db push && prisma db seed -- --environment production",
    "db:clean": "run-s db:remove db:setup",
    "db:seed-dev": "npm run db:clean && prisma db seed -- --environment development",
    "db:generate-migration": "prisma migrate dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@prisma/client": "^6.7.0",
    "@remix-run/node": "^2.16.5",
    "@remix-run/react": "^2.16.5",
    "@remix-run/serve": "^2.16.5",
    "aws-sdk": "^2.1692.0",
    "downshift": "^9.0.9",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.0.5",
    "i18next-fetch-backend": "^6.0.0",
    "i18next-fs-backend": "^2.6.0",
    "isbot": "^4.1.0",
    "meilisearch": "^0.50.0",
    "nodemailer": "^6.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-markdown": "^10.1.0",
    "remix-auth": "^4.2.0",
    "remix-auth-form": "^3.0.0",
    "remix-auth-oauth2": "^3.4.0",
    "remix-i18next": "^6.4.1",
    "remix-keycloak": "^2.0.4",
    "remix-utils": "^7.7.0",
    "tiny-invariant": "^1.3.3",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@faker-js/faker": "^9.7.0",
    "@remix-run/dev": "^2.16.5",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "remix-flat-routes": "^0.8.5",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.0.0",
    "vite-env-only": "^3.0.3",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
